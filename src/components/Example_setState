import React, { Component } from 'react';

class Ehhh extends Component {
    
    constructor(props){
        super(props)

        this.state = {
            count: 0
        }
    }
    inc = () => {
        this.setState({
            count: this.state.count + 1
        })
    }
    render() {
      return (
            <div>
              <button onClick={() => this.inc()}>{this.state.count}</button>
            </div>
          );
    }
}

export default Ehhh;